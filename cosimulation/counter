#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x13d6940 .scope module, "counter_top" "counter_top" 2 5;
 .timescale 0 0;
v0x1400b48_0 .var "clk", 0 0;
v0x1400ba0_0 .net "q", 3 0, L_0x13d6f40;  1 drivers
S_0x13a1560 .scope module, "dut" "counter" 2 10, 3 6 0, S_0x13d6940;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /OUTPUT 4 "q"
L_0x13d6f40 .functor BUFZ 4, v0x1400a20_0, C4<0000>, C4<0000>, C4<0000>;
v0x13a1678_0 .net "clk", 0 0, v0x1400b48_0;  1 drivers
v0x1400a20_0 .var "cntr", 3 0;
v0x1400a98_0 .net "q", 3 0, L_0x13d6f40;  alias, 1 drivers
E_0x13a1650 .event posedge, v0x13a1678_0;
    .scope S_0x13a1560;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x1400a20_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x13a1560;
T_1 ;
    %wait E_0x13a1650;
    %vpi_call 3 15 "$display", "from counter module, t=%5d, q=%d", $time, v0x1400a20_0 {0 0 0};
    %load/vec4 v0x1400a20_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x1400a20_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x13d6940;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1400b48_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x13d6940;
T_3 ;
    %vpi_call 2 13 "$from_myhdl", v0x1400b48_0 {0 0 0};
    %vpi_call 2 14 "$to_myhdl", v0x1400ba0_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x13d6940;
T_4 ;
    %vpi_call 2 18 "$dumpfile", "counter_top.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_top.v";
    "counter.v";
